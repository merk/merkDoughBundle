{
    "name": "merk/dough-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle integrates Dough into Symfony2",
    "keywords": ["money", "dough", "currency", "currency conversion"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Nagel",
            "email": "tim@nagel.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "symfony/form": "~2.1",
        "merk/dough": "1.*"
    },
    "require-dev": {
        "symfony/validator": "~2.1",
        "symfony/yaml": "~2.1"
    },
    "autoload": {
        "psr-0": { "merk\\DoughBundle": "" }
    },
    "target-dir": "merk/DoughBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
